{
  "name": "node-pg-knex-passport-template",
  "version": "0.0.0",
  "private": true,
  "description": "Backend Express API Template using Knex, PostgreSQL, Passport, and Typescript.",
  "keywords": [
    "example",
    "express",
    "passport",
    "knex",
    "pg",
    "template"
